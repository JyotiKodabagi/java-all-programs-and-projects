package com.oneTomany.seventhhibernateProject;

import java.util.Objects;

import jakarta.persistence.CascadeType;
import jakarta.persistence.Column;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.JoinColumn;
import jakarta.persistence.ManyToOne;
import jakarta.persistence.Table;
@Entity
@Table(name = "course")
public class Course {
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
int id;
	@Column
String course_name;
	
	@ManyToOne
	@JoinColumn(name="student_id",referencedColumnName = "id")
KodnestStudent ks;
public Course() {
	// TODO Auto-generated constructor stub
}
public Course(int id, String course_name, KodnestStudent ks) {
	super();
	this.id = id;
	this.course_name = course_name;
	this.ks = ks;
}
public Course(String course_name, KodnestStudent ks) {
	super();
	this.course_name = course_name;
	this.ks = ks;
}
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public String getCourse_name() {
	return course_name;
}
public void setCourse_name(String course_name) {
	this.course_name = course_name;
}
public KodnestStudent getKs() {
	return ks;
}
public void setKs(KodnestStudent ks) {
	this.ks = ks;
}


public Course(String course_name) {
	super();
	this.course_name = course_name;
}
@Override
public int hashCode() {
	return Objects.hash(course_name, id, ks);
}
@Override
public boolean equals(Object obj) {
	if (this == obj)
		return true;
	if (obj == null)
		return false;
	if (getClass() != obj.getClass())
		return false;
	Course other = (Course) obj;
	return Objects.equals(course_name, other.course_name) && id == other.id && Objects.equals(ks, other.ks);
}

}
